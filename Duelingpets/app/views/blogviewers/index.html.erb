<% provide(:h1, "Blogviewer List") %>
<% @blogviewers.each do |blogviewer| %>
   <div class="pageformat">
      <p>Type: <%= blogviewer.name %></p>
      <p>Created on: <%= distance_of_time_in_words_to_now blogviewer.created_on %> ago on (<%= blogviewer.created_on.strftime("%B-%d-%Y") %>)</p>
   </div>
   <br>
<% end %>
<p class="formlinks"><%= link_to 'Back', root_path %></p>
