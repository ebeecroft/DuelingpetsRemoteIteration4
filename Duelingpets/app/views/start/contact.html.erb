<% provide(:h1, "Contact") %>
<div class="pageformat">
   <%= form_for(:session, url: verify_path) do |f| %>
      <p><%= f.label :color, "What is your favorite color?" %></p>
      <p><%= f.text_field :color %></p>
      <p><%= f.submit "Submit", class: "button submitbutton" %></p>
   <% end %>
</div>
<br>
<p class="formlinks"><%= link_to "Back", root_path %></p>
