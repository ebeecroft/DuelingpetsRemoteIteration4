<% provide(:h1, "Active Users") %>
<% if @pouches.count >= 50 %>
   <div class="formlinks"><%= paginate @pouches %></div>
   <br>
<% end %>
<div class="pageformat">
   <% @pouches.each do |pouch| %>
      <div class="container">
         <p class="formlinks"><%= link_to pouch.user.vname, user_path(pouch.user) %></p>
         <p>Active last: <%= distance_of_time_in_words_to_now pouch.last_visited %></p>
      </div>
   <% end %>
</div>
<br>
<p class="formlinks"><%= link_to 'Back', root_path %></p>
