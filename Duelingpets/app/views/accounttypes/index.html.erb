<% provide(:h1, "Accounttype List") %>
<% @accounttypes.each do |accounttype| %>
   <div class="pageformat">
      <p>Name: <%= accounttype.name %></p>
      <p>Created on: <%= distance_of_time_in_words_to_now accounttype.created_on %> ago on (<%= accounttype.created_on.strftime("%B-%d-%Y") %>)</p>
   </div>
   <br>
<% end %>
<p class="formlinks"><%= link_to 'Back', root_path %></p>
